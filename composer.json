{
  "name": "interactivesolutions/honeycomb-core-ui",
  "description": "HoneyComb CMS package for ui elements",
  "keywords": ["honeycomb", "honey", "ui", "javascript", "form", "laravel"],
  "homepage": "https://github.com/interactivesolutions/honeycomb-core-ui/",
  "license": "MIT",
  "authors": [
    {
      "name": "Mantas Paulauskas",
      "email": "mp@interactivesolutions.lt"
    },
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Vytautas Rimeikis",
      "email": "vytautas.rimeikis@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "devfactory/minify": "^1.0",
    "interactivesolutions/honeycomb-core": "~1.0.0"
  },
  "require-dev": {
    "brianium/paratest": "^0.14",
    "mockery/mockery": "^0.9",
    "orchestra/testbench-browser-kit": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "interactivesolutions\\honeycombcoreui\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}
